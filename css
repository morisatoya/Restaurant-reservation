<%!
	static String fmtNull(String s){
		if(s == null || s.equals("")){
			s = "&nbsp";
		}
		return s;

	}
%>
//入力値の前後の空白を削除するJavaScript関数
function trim(str) {
	str = str.replace(/^\s+|\s+$/g, "");
	return str;
}
